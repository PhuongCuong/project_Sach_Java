	<queryString language="SQL">
		<![CDATA[SELECT "NhanVien"."maNV",
	"NhanVien"."tenNV",
	"KhachHang"."maKH",
	"KhachHang"."tenKH",
	"KhachHang"."SDT",
	"KhachHang"."diaChi",
	"CT_HoaDon"."maHoaDon",
